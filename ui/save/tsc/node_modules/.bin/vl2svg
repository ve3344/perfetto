#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules/vega-lite/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules/vega-lite/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules/vega-lite/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules/vega-lite/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/vega-lite@5.19.0_vega@5.30.0/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vega-lite/bin/vl2svg" "$@"
else
  exec node  "$basedir/../vega-lite/bin/vl2svg" "$@"
fi
