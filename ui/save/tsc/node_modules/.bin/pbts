#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules/protobufjs-cli/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules/protobufjs-cli/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules/protobufjs-cli/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules/protobufjs-cli/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/protobufjs-cli@1.1.2_protobufjs@7.3.2/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../protobufjs-cli/bin/pbts" "$@"
else
  exec node  "$basedir/../protobufjs-cli/bin/pbts" "$@"
fi
