#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules/d3-geo-projection/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/d3-geo-projection@4.0.0/node_modules:/home/runner/work/perfetto/perfetto/ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/geoproject.js" "$@"
else
  exec node  "$basedir/../../../../../d3-geo-projection@4.0.0/node_modules/d3-geo-projection/bin/geoproject.js" "$@"
fi
